{"type":"javascript","text":"import { useEffect } from 'react';\n\nexport function useTitle(title, resetOnUnmount = true) {\n  useEffect(() => {\n    if (!resetOnUnmount) return;\n    let initialTitle = document.title;\n\n    return () => {\n      document.title = initialTitle;\n    };\n  }, [resetOnUnmount]);\n\n  useEffect(() => {\n    document.title = title;\n  }, [title]);\n}","words":[{"text":"import","type":"Keyword","range":[0,6]},{"text":"useEffect","type":"Identifier","range":[9,18]},{"text":"from","type":"Identifier","range":[21,25]},{"text":"'react'","type":"String","range":[26,33]},{"text":"export","type":"Keyword","range":[36,42]},{"text":"function","type":"Keyword","range":[43,51]},{"text":"useTitle","type":"Identifier","range":[52,60]},{"text":"title","type":"Identifier","range":[61,66]},{"text":"resetOnUnmount","type":"Identifier","range":[68,82]},{"text":"true","type":"Boolean","range":[85,89]},{"text":"useEffect","type":"Identifier","range":[95,104]},{"text":"if","type":"Keyword","range":[117,119]},{"text":"resetOnUnmount","type":"Identifier","range":[122,136]},{"text":"return","type":"Keyword","range":[138,144]},{"text":"let","type":"Keyword","range":[150,153]},{"text":"initialTitle","type":"Identifier","range":[154,166]},{"text":"document","type":"Identifier","range":[169,177]},{"text":"title","type":"Identifier","range":[178,183]},{"text":"return","type":"Keyword","range":[190,196]},{"text":"document","type":"Identifier","range":[211,219]},{"text":"title","type":"Identifier","range":[220,225]},{"text":"initialTitle","type":"Identifier","range":[228,240]},{"text":"resetOnUnmount","type":"Identifier","range":[255,269]},{"text":"useEffect","type":"Identifier","range":[276,285]},{"text":"document","type":"Identifier","range":[298,306]},{"text":"title","type":"Identifier","range":[307,312]},{"text":"title","type":"Identifier","range":[315,320]},{"text":"title","type":"Identifier","range":[328,333]}]}