{"type":"typescript-jsx","text":"import { useState } from 'react';\nimport { Collapse, CollapseItem } from './Collapse';\n\nconst contentStyles = {\n  height: 300,\n\n  background: 'lightgrey',\n  display: 'flex',\n  alignItems: 'center',\n  justifyContent: 'center',\n};\n\nconst headerStyles = {\n  border: '1px solid lightgrey',\n  padding: 8,\n  cursor: 'pointer',\n};\n\nfunction App() {\n  const [openedItems, setOpenedItems] = useState<string[]>([]);\n  return (\n    <div>\n      <h1>Hello world</h1>\n\n      <Collapse\n        openedItems={openedItems}\n        onChange={setOpenedItems}\n        mode=\"accordion\"\n      >\n        <CollapseItem name=\"first\">\n          {({ headerProps, contentVisible }) => (\n            <div>\n              <div\n                {...headerProps}\n                style={{\n                  ...headerStyles,\n                  borderBottom: 'none',\n                }}\n              >\n                First collapse item\n              </div>\n              {contentVisible && <div style={contentStyles}>1</div>}\n            </div>\n          )}\n        </CollapseItem>\n\n        <CollapseItem name=\"second\">\n          {({ headerProps, contentVisible }) => (\n            <div>\n              <div {...headerProps} style={headerStyles}>\n                Second collapse item\n              </div>\n              {contentVisible && <div style={contentStyles}>2</div>}\n            </div>\n          )}\n        </CollapseItem>\n\n        <CollapseItem name=\"third\">\n          {({ headerProps, contentVisible }) => (\n            <div>\n              <div\n                {...headerProps}\n                style={{\n                  ...headerStyles,\n                  borderTop: 'none',\n                }}\n              >\n                Third collapse item\n              </div>\n              {contentVisible && <div style={contentStyles}>3</div>}\n            </div>\n          )}\n        </CollapseItem>\n      </Collapse>\n    